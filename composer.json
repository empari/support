{
    "name": "empari/support",
    "description": "Domain Tools, Utils and Support Package for Laravel",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Team #EGI",
            "email": "suporte@empari.com.br"
        }
    ],
    "require": {
        "php": "^7.0",
        "laravel/framework": "5.5.*",
        "webpatser/laravel-uuid": "^2.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Empari\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}