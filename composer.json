{
    "name": "empari/support",
    "description": "Domain Tools, Utils and Support Package for Laravel",
    "version": "1.0.8",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Team Empari",
            "email": "suporte@empari.com.br"
        }
    ],
    "require": {
        "php": "^7.0",
        "doctrine/annotations": "1.3.1",
        "doctrine/cache": "^1.7",
        "laravel/framework": "5.5.*",
        "laravelcollective/html": "^5.5",
        "webpatser/laravel-uuid": "^2.0",
        "patricktalmadge/bootstrapper": "^5.10"
    },
    "require-dev": {
        "caouecs/laravel-lang": "~3.0",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "Empari\\Support\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}